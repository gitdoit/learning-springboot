<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.seefly.springmybaits.mapper.PageMapper">
    <select id="listByName" resultType="org.seefly.springmybaits.entity.Country">
        select *
        from country
        where countryname = #{name}
    </select>

</mapper>